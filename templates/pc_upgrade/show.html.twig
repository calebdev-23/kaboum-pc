{% extends 'base.html.twig' %}

{% block title %}PcUpgrade{% endblock %}

{% block body %}
    <h1>PcUpgrade</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ pc_upgrade.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ pc_upgrade.name }}</td>
            </tr>
            <tr>
                <th>Quantite</th>
                <td>{{ pc_upgrade.quantite }}</td>
            </tr>
            <tr>
                <th>Price</th>
                <td>{{ pc_upgrade.price }}</td>
            </tr>
            <tr>
                <th>Date</th>
                <td>{{ pc_upgrade.date ? pc_upgrade.date|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Observation</th>
                <td>{{ pc_upgrade.observation }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_pc_upgrade_index') }}">back to list</a>

    <a href="{{ path('app_pc_upgrade_edit', {'id': pc_upgrade.id}) }}">edit</a>

    {{ include('pc_upgrade/_delete_form.html.twig') }}
{% endblock %}
